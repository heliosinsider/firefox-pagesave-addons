{
    "manifest_version": 2,
    "name": "AutosaveWebpage",
    "version": "1.0.0",

    "description": "Auto save any webpage",

    "icons": {
        "48": "icons/save.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "Popup.html",
        "default_title": "Autosave Webpage"
    }
}